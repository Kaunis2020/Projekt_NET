<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/produktstil.css" />
    <link rel="stylesheet" href="~/css/modalstil.css" />
    <link rel="stylesheet" href="~/css/formstil.css" />
    <link rel="stylesheet" href="~/css/tablestil.css" />
    <link rel="stylesheet" href="~/css/jquery-ui.css" />
    <link rel="stylesheet" href="~/css/jquery-ui.theme.min.css" />
    <link rel="stylesheet" href="~/css/bootstrap-theme.css" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/display.js"></script>
</head>
<body>
    <audio id="nyaudio" preload="auto" controls volume="1.0">
        <source src="~/audio/error.mp3" type="audio/mpeg">
    </audio>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink(" Bonzi's Mediabutik ", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink(" Home ", "Index", "Home")</li>
                    <li>@Html.ActionLink(" Media ", "OurPagMedia", "Media")</li>
                    <li>@Html.ActionLink(" Sök ", "SearchMedia", "Media")</li>
                    <li>@Html.ActionLink(" Om oss ", "About", "Home")</li>
                    <li>@Html.ActionLink(" Medarbetare ", "allOurEmployees", "Home")</li>
                    <li>@Html.ActionLink(" Kontakt ", "Contact", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div>
        @RenderBody()
    </div>
    <br />
    <br />
    <div>
        <footer class="egenfooter">
            <hr />
            <p><a href="@Url.Action("AdminLogin", "Admin")"><img id="lockdiv" src="~/images/lockico.png" alt="lock" /></a></p>
            <p id="footerright">&copy; @DateTime.Now.Year -- Bonzi's Mediabutik AB</p>
        </footer>
        <br />
    </div>
<br/>
<br/>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
